{
  "name": "vscode-webview",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "translatol",
  "sourceRoot": "apps/vscode-webview/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/vscode-extension/webview",
        "index": "apps/vscode-webview/src/index.html",
        "main": "apps/vscode-webview/src/main.ts",
        "polyfills": ["@angular/localize/init", "zone.js"],
        "tsConfig": "apps/vscode-webview/tsconfig.app.json",
        "assets": [
          "apps/vscode-webview/src/favicon.ico",
          {
            "glob": "**/*.*",
            "input": "apps/vscode-webview/src/assets",
            "output": "assets"
          },
          {
            "glob": "**/*.*",
            "input": "node_modules/@vscode/codicons/dist",
            "output": "assets/codicons"
          }
        ],
        "styles": [
          "node_modules/@clr/ui/clr-ui.min.css",
          "libs/shared-module/src/assets/styles.scss",
          "apps/vscode-webview/src/styles.scss"
        ],
        "scripts": [],
        "allowedCommonJsDependencies": ["lodash", "@vtabary/xliff2js"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "10mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "none",
          "buildOptimizer": true,
          "optimization": true
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "vscode-webview:build:production"
        },
        "development": {
          "browserTarget": "vscode-webview:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "vscode-webview:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/vscode-webview/**/*.ts",
          "apps/vscode-webview/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/vscode-webview/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "vscode-webview:build"
      }
    }
  }
}
